{
    "title": "My title",
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": [
            "."
        ],
        "exclude": [
            "node_modules"
        ]
    },
    "tags": {
        "allowUnknownTags": [
            "category"
        ]
    },
    "plugins": [
        "node_modules/better-docs/category"
    ],
    "recurseDepth": 10,
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "template": "node_modules/better-docs",
        "readme": "README.md"
    },
    "templates": {
        "better-docs": {
            "name": "Open Inventory - Backend Source API Reference",
            "navigation": [
                {
                    "label": "Github",
                    "href": "https://github.com/openinventorysystem/backend-api"
                },
                {
                    "label": "Complete Documentation",
                    "href": "https://openinventorysystem.github.io/docs/"
                }
            ]
        }
    }
}