{
    "title": "My title",
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": [
            "."
        ],
        "exclude": [
            "node_modules"
        ]
    },
    "tags": {
        "allowUnknownTags": [
            "category"
        ]
    },
    "recurseDepth": 10,
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "readme": "README.md",
        "template": "node_modules/clean-jsdoc-theme"
    }
}